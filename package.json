  },
  "dependencies": {
    "@vant/weapp": "^1.8.3"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-less": "^5.0.0",
    "gulp-rename": "^2.0.0",
